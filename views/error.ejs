<%layout('layouts/boilerplate')%>

<div class="alert alert-danger" role="alert">
    <h2 class="alert-heading">ERROR!!!!</h2>
    <h3> <%= err.message %> </h3>
    <% if(process.env.NODE_ENV !== "production"){ %>
        <p>err.stack</p>
    <% } %>
    <p> Now that you have seen the error message do something</p>
</div>